
@{
    ViewBag.Title = "Non-Lot General Report";
    Layout = "~/Views/Shared/_LayoutRptVue.cshtml";
}


<link href="~/Content/style.css" rel="stylesheet">
<link href="~/themes/local/lotandfoup.css" rel="stylesheet">
<link rel="stylesheet" href="~/Content/plugins/element/index.css">

<script src="~/Scripts/vue.min.js"></script>
<script src="~/Scripts/plugins/element/index.js"></script>

<el-container>
    <el-header height="700">
        <el-collapse v-model="activeNames">
            <el-collapse-item title="查询条件" name="1">
                <el-row type="flex">
                    <el-col :lg="4" :md="12" :sm="12">
                        <div><label>Department</label></div>
                        <el-select multiple v-model="departments" placeholder="默认全选"></el-select>
                    </el-col>
                    <el-col :lg="8" :md="12" :sm="24" offset="2">
                        <div><label>EqpType</label></div>
                        <el-select multiple v-model="eqptypes" placeholder="默认全选"></el-select>
                    </el-col>


                    <el-col :lg="5" :offset="2" :sm="12">
                        <el-card class="box-card">
                            <div>开发者：曹晋（0279）</div>
                            <div>需求者：陈舒（0353）</div>
                            <div>Cilck Count: @ViewBag.ClickCount</div>
                        </el-card>
                    </el-col>
                </el-row>
            </el-collapse-item>
        </el-collapse>
    </el-header>

    <el-main>
        <div class="html5buttons">
            <div class="dt-buttons btn-group">
                <a class="btn btn-default buttons-excel buttons-html5" tabindex="1"
                   aria-controls="DataTables_Table_0" v-on:click.stop="outputExcel">
                    <span>
                        <img src="~/themes/icons/excel_2007.gif" />
                    </span>
                </a> <a ref="dlink" style="display: none;"></a>
            </div>
        </div>
        <label>Lot Infomation</label>
        <el-table :data="lotEntities" border size="mini" style="width: 100%" stripe max-height="300">
            <el-table-column type="index" width="50" align="center"></el-table-column>
            <el-table-column property="Lot_ID" label="LotID" align="center"></el-table-column>
            <el-table-column property="Qty" label="Q'ty" align="center"></el-table-column>
            <el-table-column property="lotOwnerId" label="Lot Owner" align="center"></el-table-column>
            <el-table-column property="lotOwnerName" label="Lot Owner Name" align="center"></el-table-column>
            <el-table-column property="Cast_ID" label="Foup ID" align="center"></el-table-column>
            <el-table-column property="Foup_Owner" label="Foup Owner" align="center"></el-table-column>
            <el-table-column property="Foup_Owner_Name" label="Foup Owner Name" align="center"></el-table-column>
        </el-table>
        <br>
        <label>Foup Infomation</label>
        <el-table :data="foupEntities" border size="mini" style="width: 100%" stripe max-height="300">
            <el-table-column type="index" width="50" align="center"></el-table-column>
            <el-table-column property="Cast_ID" label="Foup ID" align="center"></el-table-column>
            <el-table-column property="foupOwnerId" label="Foup Owner" align="center"></el-table-column>
            <el-table-column property="foupOwnerName" label="Foup Owner Name" align="center"></el-table-column>
        </el-table>
    </el-main>

</el-container>


<script src="~/Scripts/ams.commontool.js"></script>
<script src="~/Scripts/demo/nonlot.js"></script>
